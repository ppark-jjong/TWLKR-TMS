# deploy/local/.env.local
# 데이터베이스 설정
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=1234
MYSQL_DATABASE=delivery_system

# 서버 설정
API_PORT=8000
DEBUG=True

# 로깅 설정
LOG_LEVEL=DEBUG
ENABLE_ACCESS_LOG=True

# 캐시 설정
CACHE_EXPIRATION=3600
SESSION_EXPIRE_HOURS=4

# 프론트엔드 설정
REACT_APP_API_URL=http://localhost:8000
NODE_ENV=development
PORT=3000
