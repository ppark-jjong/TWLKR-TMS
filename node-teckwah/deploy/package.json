{
  "name": "teckwah-dashboard",
  "version": "1.0.0",
  "description": "Teckwah Dashboard Application",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "winston": "^3.10.0",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "exceljs": "^4.3.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "client": {
    "dependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.10.0",
      "@tanstack/react-query": "^4.29.5",
      "axios": "^1.4.0",
      "antd": "^5.4.2",
      "chart.js": "^4.4.1",
      "dayjs": "^1.11.7",
      "jwt-decode": "^3.1.2",
      "recharts": "^2.5.0"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
